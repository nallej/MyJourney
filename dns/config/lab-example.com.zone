$TTL 2d

$ORIGIN lab.example.com.

@               IN      SOA     ns.lab.example.com. admin.example.com (
                                2022122800      ; serial
                                12h             ; refresh
                                15m             ; retry
                                3w              ; expire
                                2h              ; minimum ttl
                                )

                IN      NS      ns.lab.example.com.

ns              IN      A       192.168.1.3

